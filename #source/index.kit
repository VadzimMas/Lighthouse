<!DOCTYPE html>
<html lang="ru">

<head>
     <title>Главная</title>
     <meta charset="UTF-8" />
     <link rel="icon"
           href="img/icons/favicon.ico"
           type="image/gif"
           sizes="16x16">

     <link rel="stylesheet"
           href="css/style.css">

     <link rel="preload"
           as="script"
           href="js/main.js"
           onload="var script = document.createElement('script');
                                     script.src = this.href;
                                     script.setAttribute('defer','');
                                     document.body.appendChild(script);">

     <meta name="viewport"
           content="width=device-width, initial-scale=1.0" />

</head>

<body>
     <div class="wrapper">
          <div class="container">
               <ul class="parralax"
                   id="scene">

                    <li class="parralax__bg"
                        data-depth="0.4">
                         <div class="img"></div>
                         <div class="light-1-wrapper">
                              <div class="light-1"></div>
                         </div>
                         <div class="light-2-wrapper">
                              <div class="light-2"></div>
                         </div>
                         <div class="light-3-wrapper">
                              <div class="light-3"></div>
                         </div>

                    </li>


                    <li class="parralax__rope-1"
                        data-depth="0.8">
                         <div class="rope1-wrapper">
                              <img class="rope"
                                   src="img/rope.png"></img>
                              <img class="birds-1"
                                   src="img/clouds/board-birds.png"></img>
                              <img class="cloud-1"
                                   src="img/clouds/board-cloud-1.png"></img>
                              <img class="cloud-2"
                                   src="img/clouds/board-cloud-3.png"></img>
                              <img class="birds-2"
                                   src="img/clouds/board-birds.png"></img>
                              <img class="cloud-3"
                                   src="img/clouds/board-cloud-2.png"></img>
                              <img class="cloud-4"
                                   src="img/clouds/board-cloud-3.png"></img>
                         </div>
                    </li>
                    <li class="parralax__rope-2"
                        data-depth="1">
                         <div class="rope2-wrapper">
                              <img class="rope"
                                   src="img/rope.png"></img>
                              <img class="cloud-1"
                                   src="img/clouds/board-cloud-4.png"></img>
                              <img class="cloud-2"
                                   src="img/clouds/board-cloud-1.png"></img>
                              <img class="cloud-3"
                                   src="img/clouds/board-cloud-2.png"></img>
                         </div>
                    </li>
                    <li class="parralax__rope-3"
                        data-depth="1.2">
                         <div class="rope3-wrapper">
                              <img class="rope"
                                   src="img/rope.png"></img>
                              <img class="birds"
                                   src="img/clouds/board-birds.png"></img>
                              <img class="cloud-1"
                                   src="img/clouds/board-cloud-2.png"></img>
                              <img class="cloud-2"
                                   src="img/clouds/board-cloud-1.png"></img>
                              <img class="cloud-3"
                                   src="img/clouds/board-cloud-4.png"></img>
                         </div>
                    </li>

                    <li class="parralax__wave-1 wave"
                        data-depth="0.6">
                         <div class="img-w">
                              <div class="black"></div>
                         </div>
                    </li>
                    <li class="parralax__wave-2 wave"
                        data-depth="0.8">
                         <div class="img-w">
                              <div class="black"></div>
                         </div>
                    </li>
                    <li class="parralax__wave-3 wave"
                        data-depth="1">
                         <div class="img-w">
                              <div class="black"></div>
                         </div>
                    </li>

                    <li class="parralax__liighthouse"
                        data-depth="1">

                         <div class="lighthouse-wrapper">
                              <div class="lighthouse">
                                   <div class="light"></div>
                              </div>
                         </div>


                    </li>

                    <li class="parralax__wave-4 wave"
                        data-depth="1.2">
                         <div class="img-w">
                              <div class="black"></div>
                         </div>
                    </li>
                    <li class="parralax__wave-5 wave"
                        data-depth="1.4">
                         <div class="img-w">
                              <div class="black"></div>
                         </div>
                    </li>
                    <li class="parralax__wave-6 wave"
                        data-depth="1.6">
                         <div class="img-w"></div>
                    </li>
               </ul>
          </div>
     </div>


</body>

</html>
